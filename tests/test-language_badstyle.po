# 불필요한 복수형
msgid "Which ones would you like to save?"
msgstr "어떤 것(들)을 저장할까요?"

# 불필요한 복수형 2 - 알려진 명사
msgid "files"
msgstr "파일들"

# 불필요한 복수형 3 - 들+조사
msgid "do blahblah things this"
msgstr "이것은 뭐시기거시기들을 뭐뭐한다"

# 불필요한 복수형 예외
msgid "turn the handle"
msgstr "핸들을 돌려"

# ...하기 위해
msgid "do something to do what"
msgstr "뭐하기 위해 뭐합니다"

# ...와 함께
msgid "do it with something"
msgstr "무언가와 함께 뭐하십시오"

# ...에 의해
msgid "done by something"
msgstr "무언가에 의해 됐습니다"

# 필요로 하다
msgid "this needs something"
msgstr "이것이 무언가를 필요로 합니다"

# ...할 것입니다
msgid "this will do something"
msgstr "무언가를 해버릴 것입니다"

# 예외: 과거형
msgid "this will do something"
msgstr "무언가가 잘못됐을 것입니다"

# ...하시기 바랍니다
msgid "Please do blahblah"
msgstr "블라블라 하시기 바랍니다"

# 몇개의 아무개
msgid "Copying %d files"
msgstr "%d개의 파일을 복사 중"

# ...을 포함
msgid "It has something containing another"
msgstr "다른 무언가를 포함하는 무언가가 있습니다"
